    "panelTitle.activeBorder": "#FF0000",

    "editorWidget.border": "#990000",
    "editorGroupHeader.border": "#990000",

    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF",
    "terminal.border": "#990000",
    "terminal.ansiRed": "#FF3333",
    "terminal.ansiBrightRed": "#FF6666",

    "settings.focusedRowBackground": "#111111",
    "settings.modifiedItemIndicator": "#990000",
    "input.background": "#000000",
    "input.foreground": "#FFFFFF",
    "input.border": "#990000",
    "input.placeholderForeground": "#CCCCCC",
    "inputValidation.infoBackground": "#000000",
    "inputValidation.infoForeground": "#FFFFFF",
    "inputValidation.infoBorder": "#990000",
    "inputValidation.warningBackground": "#000000",
    "inputValidation.warningForeground": "#FFFFFF",
    "inputValidation.warningBorder": "#990000",
    "inputValidation.errorBackground": "#000000",
    "inputValidation.errorForeground": "#FFFFFF",
    "inputValidation.errorBorder": "#990000",

    "dropdown.background": "#000000",
    "dropdown.foreground": "#FFFFFF",
    "dropdown.border": "#990000",

    "focusBorder": "#990000",
    "inputOption.activeBorder": "#990000"
