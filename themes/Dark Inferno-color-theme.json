{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Inferno",
  "base": "vs-dark",
  "type": "dark",
  "colors": {
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#CCCCCC",
    "titleBar.border": "#990000",

    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#CCCCCC",
    "tab.border": "#990000",
    "tab.activeBorderTop": "#FF3333",
    "tab.unfocusedActiveBorderTop": "#660000",
    "tab.unfocusedActiveBorder": "#660000",

    "window.activeBorder": "#990000",
    "window.inactiveBorder": "#660000",

    "button.background": "#990000",
    "button.hoverBackground": "#CC0000",

    "list.activeSelectionBackground": "#990000",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#660000",
    "list.hoverBackground": "#CC3333",

    "editor.background": "#000000",
    "editor.selectionBackground": "#992222",
    "editor.selectionHighlightBackground": "#66000088",
    "editor.lineHighlightBackground": "#111111",
    "editorLineNumber.activeForeground": "#FF3333",
    "editorRuler.foreground": "#990000",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.border": "#990000",
    "activityBarBadge.background": "#CC0000",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.activeBorder": "#FF0000",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.border": "#990000",
    "statusBar.debuggingBackground": "#660000",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBarItem.remoteBackground": "#e70000",
    "statusBarItem.remoteForeground": "#FFFFFF",

    "notifications.background": "#000000",
    "notifications.foreground": "#FFFFFF",
    "notifications.border": "#990000",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#FFFFFF",
    "sideBar.border": "#990000",
    "sideBarTitle.foreground": "#FFFFFF",
    "sideBarSectionHeader.background": "#111111",
    "sideBarSectionHeader.foreground": "#FF3333",

    "editorGroup.border": "#990000",
    "editorGroupHeader.tabsBackground": "#000000",
